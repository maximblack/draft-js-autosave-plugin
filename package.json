{
  "name": "draft-js-autosave-plugin",
  "version": "0.0.0",
  "description": "Autosave Plugin for DraftJS Plugins Editor",
  "author": {
    "name": "Conner Jevning",
    "url": "https://github.com/cjevning"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/synocate/draft-js-autosave-plugin.git"
  },
  "main": "lib/index.js",
  "keywords": [
    "editor",
    "wysiwyg",
    "draft",
    "react",
    "ux",
    "components",
    "widget",
    "react-component"
  ],
  "peerDependencies": {
    "draft-js-plugins-editor": "2.0.0-beta10",
    "react": "^15.0.0"
  },
  "scripts": {
    "clean": "rimraf lib",
    "build": "npm run clean && npm run build:js && npm run build:css",
    "build:js": "BABEL_DISABLE_CACHE=1 BABEL_ENV=production NODE_ENV=production babel --out-dir='lib' --ignore='__test__/*' src",
    "build:css": "node ./scripts/concatCssFiles $(pwd) && ./node_modules/.bin/rimraf lib-css",
    "prepublish": "npm run build",
    "test": "mocha --compilers js:babel-core/register --require jsdom-global/register **/__test__/*.js"
  },
  "license": "MIT",
  "dependencies": {
    "decorate-component-with-props": "^1.0.2",
    "draft-js": "~0.10.0"
  },
  "devDependencies": {
    "babel-cli": "^6.18.0",
    "babel-eslint": "^7.2.1",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-webpack-loaders": "^0.9.0",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-react": "^6.16.0",
    "babel-preset-react-hmre": "^1.1.1",
    "babel-preset-stage-0": "^6.16.0",
    "chai": "^3.5.0",
    "chai-enzyme": "^0.6.1",
    "draft-js-plugins-editor": "2.0.0-beta9",
    "enzyme": "^2.6.0",
    "eslint": "^3.18.0",
    "eslint-config-airbnb": "^14.1.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^4.0.0",
    "eslint-plugin-mocha": "^4.9.0",
    "eslint-plugin-react": "^6.10.3",
    "eslint-plugin-standard": "1.3.2",
    "extract-text-webpack-plugin": "^2.1.0",
    "file-loader": "^0.10.1",
    "jsdom": "^9.8.3",
    "jsdom-global": "^2.1.0",
    "mocha": "^3.2.0",
    "react": "^15.4.1",
    "react-addons-test-utils": "^15.4.1",
    "react-dom": "^15.4.1",
    "rimraf": "^2.5.4",
    "sinon": "^1.17.6",
    "sinon-chai": "^2.8.0",
    "style-loader": "^0.16.0"
  }
}
